#include <cstring>

#include "aimrt_pkg_c_interface/pkg_macro.h"
#include "pb_server_module/pb_server_module.h"
#include "ros2_server_module/ros2_server_module.h"

static std::tuple<std::string_view, std::function<aimrt::ModuleBase*()>>
    aimrt_module_register_array[]{
        {"PbServerModule",
         []() -> aimrt::ModuleBase* {
           return new rpc::pb_server_module::PbServerModule();
         }},
        {"Ros2ServerModule",
         []() -> aimrt::ModuleBase* {
           return new rpc::ros2_server_module::Ros2ServerModule();
         }},
    };

AIMRT_PKG_MAIN(aimrt_module_register_array)
